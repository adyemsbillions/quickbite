{
  "expo": {
    "name": "cravii",
    "slug": "quickbite",
    "scheme": "cravii",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo.png",
    "splash": {
      "image": "./assets/logo.png",
      "resizeMode": "cover",
      "backgroundColor": "#000000"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.adyemsbillions.quickbite", 
      "buildNumber": "1.0.0" 
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logo.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.adyemsbillions.quickbite",
      "versionCode": 1,
      "googleServicesFile": "./google-services.json" 
    },
    "extra": {
      "eas": {
        "projectId": "208ab134-79af-456a-9178-4e4da5a3fb8e"
      }
    },
    "owner": "adyemsbillions",
    "plugins": [
      "expo-router",
      [
        "expo-notifications",
        {
          "icon": "./assets/images/notification-icon.png", 
          "color": "#ff5722", 
          "sounds": ["./assets/sounds/notification.wav"] 
        }
      ]
    ]
  }
}